graph {
	node [shape=box]

	e1 -- e2, plus1, e3
	e1 [label = "E"]

	e2 -- int1
	e2 [label = "E"]

	int1 [label = <<i>Int<sub>t</sub></i>("1")>]

	plus1 [label = <<i>Plus<sub>t</sub></i>(" + ")>]

	e3 -- e4, star1, e5
	e3 [label = "E"]

	e4 -- int2
	e4 [label = "E"]

	int2 [label = <<i>Int<sub>t</sub></i>("2")>]

	star1 [label = <<i>Star<sub>t</sub></i>(" * ")>]

	e5 -- int3
	e5 [label = "E"]

	int3 [label = <<i>Int<sub>t</sub></i>("3")>]
}

